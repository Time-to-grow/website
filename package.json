{
    "name": "website",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --open",
        "ndev": "netlify env:import netlify/functions/.env && netlify dev",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint ./src --ext .ts,.tsx",
